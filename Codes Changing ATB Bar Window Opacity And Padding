Window_SATBBar.prototype.initialize = function(t) {
    this.setBattler(t);
    var i = {
        x: this._x(),
        y: this._y(),
        width: this._w(),
        height: this._h()
    };
    Window_Base.prototype.initialize.call(this, i.x, i.y, i.width, i.height), this.opacity = 0
}, Window_SATBBar.prototype.standardPadding = function() {
    return 0
};
